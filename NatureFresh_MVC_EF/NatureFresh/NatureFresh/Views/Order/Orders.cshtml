@model dynamic
@{
    ViewBag.Title = "Orders";
}

<h2>Orders</h2>

@foreach(var i in Model)
{
    <div class="row bg-danger">
        <div class="col-md-2 btn col-md-offset-2 btn-info">Date: @i.Date</div>
        <div class="col-md-2 btn col-md-offset-3 btn-primary">Total: @i.TotalPrice</div>
        <br /><br />
    </div>
}